
@{
    //Layout = null;

    List<String> Posts = new List<string>();
    Posts = (List<string>)Session["Posts"];

}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>

    <div> 
        @{ 
            for (int i = 0; i < Posts.Count; i++)
            {
                <a href="/posts/@i">@Posts[i]</a>
            }
        }
        <br /><br />
        <a href="/post/create">Create new Post</a>
        @*<a href="/post/0">1</a>
        <a href="/post/1">2</a>
        <a href="/post/2">3</a>*@
    </div>
</body>
</html>
